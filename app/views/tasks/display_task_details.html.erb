<h1>Tasks</h1>

<table border = '1'>
  <thead>
    <tr>
      <th> Task Name </th>
    </tr>
  </thead>

  <tbody>
    <% @tasks.each do |task| %>
      <tr>
        <td><%= link_to task.task_name, task_comment_page_tasks_path(:task_id => task.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table> <br/>

<div>
  <p> <%= link_to "dashboard", dashboard_index_url %> </p>
</div>

<br>